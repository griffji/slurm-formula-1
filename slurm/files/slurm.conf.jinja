# Dynamic slurm.conf file generated by salt-minion(1)
# DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN
{% from "slurm/map.jinja" import slurm with context %}

## BASE ##
{%- for key, value in slurm.get("global").iteritems() %}
{{ key }}={{ value -}}
{% endfor %}


## TIMERS ##
{%- for key, value in slurm.get("timers").iteritems() %}
{{ key }}={{ value -}}
{% endfor %}


## SCHEDULING ##
{%- for key, value in slurm.get("scheduling").iteritems() %}
{{ key }}={{ value -}}
{% endfor %}


## LOGGING AND ACCOUNTING ##
{%- for key, value in slurm.get("logging_accounting").iteritems() %}
{{ key }}={{ value -}}
{% endfor %}


## COMPUTE NODES ##
{%- for key, value in slurm.get("node_config", {}).iteritems() %}
{{ key }}={{ value -}}
{% endfor %}
{%- for nodename, settings in slurm.get("nodes").iteritems() %}
NodeName={{ nodename }} {% for key, value in settings.iteritems() %}{{ key }}={{ value }} {% endfor -%}
{% endfor %}
{%- for partitionname, settings in slurm.get("partitions").iteritems() %}
PartitionName={{ partitionname }} {% for key, value in settings.iteritems() %}{{ key }}={{ value }} {% endfor -%}
{% endfor %}
